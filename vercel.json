{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api.py"
        },
        {
            "src": "/(.*)",
            "dest": "backend/api.py"
        }
    ],
    "env": {
        "PYTHONPATH": "./backend"
    },
    "functions": {
        "backend/api.py": {
            "maxDuration": 30
        }
    }
}